title = {{Stock-Flow Consistent Modeling Through the Ages}},
url = {http://papers.ssrn.com/abstract=2196498$\backslash$nhttp://papers.ssrn.com/sol3/papers.cfm?abstract\_id=2196498},
year = {2013}
}
