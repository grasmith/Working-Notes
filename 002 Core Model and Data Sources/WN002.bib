@article{Stockhammer1999,
abstract = {The aim of the paper is to give an overview over basic models of Post-Keynesian growth theory. Two major families of growth models are discussed, one developed by Joan Robinson, the other by Michal Kalecki. Both share an independent investment function that depends on income distribution and a savings function that depends on income distribution. The core difference that the Robinsonian model assumes full capacity utilization in the long run, while the Kaleckian model has capacity utilization as an endogenous variable. The characteristics of these models and in particular the effects of changes in the savings propensity and the relation between distribution and growth are highlighted and contrasted. A short run Keynes-Kalecki model is as a benchmark case.},
author = {Stockhammer, Engelbert},
