@article{Barwell2011,
abstract = {The years leading up to the financial crisis are widely acclaimed as a period of remarkable, if not$\backslash$nunprecedented, stability in the global economy. From a perspective that focuses on the value and$\backslash$nvolume of gross domestic product, that assessment is valid. The world's major economies enjoyed a$\backslash$nsustained period of growth. Inflation was low and stable. But from a perspective that focuses on$\backslash$nthe value of financial assets and on the volume of market activity, this period was anything but$\backslash$nstable. The past decade has borne witness to sharp swings in asset prices and a large expansion in$\backslash$ncredit and balance sheets. This paper considers a flow-of-funds approach that stresses the role of$\backslash$nasset prices, credit and balance sheets. This framework is used to shine a different light on the$\backslash$nGreat Moderation in the United Kingdom. It suggests that there were linkages between many of the$\backslash$nmacroeconomic puzzles of the day and the balance sheet developments that led to financial$\backslash$ninstability. It further argues that approaches to macroeconomics that stress the importance of$\backslash$nbalance sheet linkages might be helpful in spotting building financial fragility.},
author = {Barwell, Richard and Burrows, Oliver},
file = {:C$\backslash$:/Users/graeme/Documents/Mendeley Desktop/Barwell, Burrows - 2011 - Growing Fragilities Balance Sheets in the Great Moderation.pdf:pdf},
journal = {Bank of England Financial Stability Paper No. 10},
keywords = {SFC,financial stability,great moderation,stock flow consistent models},
mendeley-tags = {SFC},
number = {10},
pages = {35},
publisher = {Bank of England},
title = {{Growing Fragilities? Balance Sheets in the Great Moderation}},
url = {http://www.bankofengland.co.uk/publications/fsr/fs\_paper10.pdf},
year = {2011}
}
@unpublished{Batini2012,
abstract = {The output effects of 2009 fiscal expansions have been hotly debated. But the discussion of fiscal multipliers is even more relevant now that several European countries have had to quickly retract their stimulus measures in an effort to regain market confidence. Using regime-switching VARs we estimate the impact of fiscal adjustment on the United States, Europe and Japan allowing for fiscal multipliers to vary across recessions and booms. We also estimate ex ante probabilities of recessions derived in association with different-sized and different types of consolidation shocks (expenditure-versus tax-based). We use these estimates to understand how consolidations should be designed to be most effective in terms of permanently and rapidly reducing a country's debt-to-GDP ratio. The main finding is that smooth and gradual consolidations are to be preferred to frontloaded or aggressive consolidations, especially for economies in recession facing high risk premia on public debt, because sheltering growth is key to the success of fiscal consolidation in these cases.},
address = {Washington, DC},
author = {Batini, Nicoletta and Callegari, Giovanni and Melina, Giovanni},
booktitle = {IMF Working Papers},
doi = {10.5089/9781475505382.001},
