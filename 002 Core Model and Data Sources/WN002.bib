mendeley-tags = {macroprudential},
number = {284},
pages = {44},
series = {BIS Working Papers},
title = {{Towards an operational framework for financial stability : “ fuzzy ” measurement and its consequences}},
url = {http://papers.ssrn.com/sol3/papers.cfm?abstract\_id=1458294},
year = {2009}
}
