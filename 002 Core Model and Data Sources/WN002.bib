@article{Officer1986,
abstract = {In contrast to conventional, disaggregative testing of the law of one price, a study is conducted that adopts an aggregative approach. The premise of the study is that the reason for the failure of the law of one price in empirical testing is the disaggregative approach almost uniformly followed. First, an equation is developed involving purchasing power parity, the exchange rate, and the nontradable/tradable price ratio in each country. Next, this relationship is used to form 2 tests of the law of one price: 1. equality of the prices of tradables and nontradables for a given country, and 2. equality of the price of tradables across countries. Tests for the validity of the aggregative approach are described as well. The disaggregative approach shows biases that lead to rejection of the law of one price. In contrast, the results of the aggregative method for the law of one price are strongly positive, and the limitations of the approach are found not to overcome its findings.},
author = {Officer, Lawrence H},
journal = {Journal of Macroeconomics},
pages = {159 -- 182},
title = {{The law of one price cannot be rejected: Two tests based on the tradable/nontradable price ratio}},
volume = {8},
year = {1986}
}
@article{Oreiro2005,
